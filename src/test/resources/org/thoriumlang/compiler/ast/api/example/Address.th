use Country; // FIXME should not be mandatory
use java.lang.String;

class Address {
    val street: String;
    val zipCode: String;
    val country: Country;

    public getStreet(): String {
        return street;
    }

    public getZipCode(): String {
        return zipCode;
    }

    public getCountry(): Country {
        return country;
    }
}